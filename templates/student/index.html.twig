          <!-- Collapsible wrapper -->
        </div>
    </div>
    <!-- Container wrapper -->
    </nav>
  </main>
  <!--Main layout-->
   
<!--Second layout-->


<div class="container">
 <h1 class="form-title">Student</h1>
<table class="table class-table">
  <thead>
    <tr>
      <th>#</th>
      <th>Name</th>
      <th>Sex</th>
      <th>Phone</th>
      <th>Date Of Birth</th>
      <th>Images</th>
    </tr>
  </thead>
  <tbody>
    {% for id in student %}
    <tr>
      <td>{{ id.id }}</td>
      <td>{{ id.name }}</td>
      <td>{{ id.sex }}</td>
      <td>{{id.phone}}</td>
      <td>{{ variable|format_datetime(id.dateofbirth) }}</th>
     <div class="pt-3">
      {% set img = form.image.vars.value %}
      {% if img %}
        <img src="{{asset('uploads/'~img)}}"
          class="w-25 shadow p-3 mb-5 bg-body rounded" />
      {% else %}
        <img src="https://cdn2.iconfinder.com/data/icons/symbol-blue-set-3/100/Untitled-1-94-512.png"
          class="w-25 shadow p-3 mb-5 bg-body rounded" />
      {% endif %}                                  
      </div>
        <button class="btn-primary" href="{{ path('class_show', { 'id': id.id }) }}">show</button>
      </td>
    </tr>
    {% endfor %}
  </tbody>
</table>

  <style>
  .container {
  max-width: 1500px;
  margin: 0 auto;
}
  .btn-primary {
  background-color: gray;
  color: white;
  padding: 10px 20px;
  border: none;
  border-radius: 5px;
  }
  </style>
{% endblock %}
<!--Second layout-->
    


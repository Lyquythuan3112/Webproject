{% extends 'base.html.twig' %}
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"></script>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.2/font/bootstrap-icons.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css"/>

{% block title %}Hello MainController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div< class="example-wrapper">
    <form action="/" method="get">
    <div class="btn-group" role="group" aria-label="Basic example">
        <button type="submit" class="btn btn-secondary">HomePage</button>
        <button type="submit" class="btn btn-secondary">Student</button>
        <button type="submit" class="btn btn-secondary">Class</button>
        <button type="submit" class="btn btn-secondary">Subject Management</button>
        <button type="submit" class="btn btn-secondary">Point management</button>
        <button type="submit" class="btn btn-secondary">Instructor Management</button>
        <button type="submit" class="btn btn-secondary">Logout</button>
        <button type="submit" class="btn btn-secondary" formmethod="POST">Login</button>
        <button type="submit" class="btn btn-secondary" formmethod="POST">Register</button>
    </div>
    </form>
</div>
{% endblock %}

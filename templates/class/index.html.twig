<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <title>{% block title %}Student Management{% endblock %}</title>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css" />
    <!-- Google Fonts Roboto -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700 &display=swap" />
    <!-- MDB -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.1/jquery.min.js"
        integrity="sha512-aVKKRRi/Q/YV+4mjoKBsE4x3H+BkegoM/em46NNlCqNTmUYADjBbeNefNxYV7giUp0VxICtqdrbqU7iVaeZNXA=="
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <!-- Custom styles -->
    <link rel="stylesheet" type="text/css" href="style.css" />
</head>

<body>

    <!-- Navbar -->
    <nav class="navbar fixed-top navbar-expand-lg navbar-light bg-white">
        <!-- Container wrapper -->
        <div class="container">
            <!-- Toggle button -->
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent1">
                <i class="fas fa-bars"></i>
            </button>

            <!-- Collapsible wrapper -->
            <div class="collapse navbar-collapse" id="navbarSupportedContent1">
                <!-- Navbar brand -->
                <a class="navbar-brand mt-2 mt-sm-0" href="#">
                    <img src="https://png.pngtree.com/thumb_back/fw800/background/20210115/pngtree-abstract-gear-mechanism-background-image_528721.jpg"
                        height="50" alt="Logo" loading="lazy" />
                </a>
                <!-- Left links -->
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item active">
                        <a class="nav-link " href="#">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="">Class</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="">Student</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="">Subject</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="">Point</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="">Instructor</a>
                    </li>
                </ul>
            </div>
            {% extends 'base.html.twig' %}

            Show Class

            {% block body %}
            <style>
                .posts {
                    border-radius: 10px;
                    box-shadow: 0 10px 16px rgba(0, 0, 0, .25);
                    margin-bottom: 40;
                    margin-top: 40px;
                    padding: 40px;
                }

                img {
                    border-radius: 10px;
                }
            </style>
            {% for message in app.flashes('success') %}
            <div class="alert alert-success mt-5 text-center">
                {{ message }}
            </div>
            {% endfor %}
            {% for ?3 in ?2 %}
            <section class="posts">
                <article>
                    <h2>?4</h2>
                    <div class="metadata">
                        ?5 by ?6
                    </div>
                    <div class="content">
                        <p>?7</p>
                        <a href="?8">Read more</a>
                    </div>
                </article>
            </section>
            {% endfor %}

            {% endblock %}
        </div>
    </nav>
</body>